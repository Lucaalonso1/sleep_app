{
  "expo": {
    "name": "Sleep App",
    "slug": "sleep-app",
    "owner": "lukfy05",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sleep-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#1e293b"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sleepapp.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ],
        "NSMicrophoneUsageDescription": "Esta app necesita acceso al micr칩fono para grabar sonidos del sue침o"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1e293b"
      },
      "package": "com.sleepapp.app",
      "permissions": [
        "android.permission.VIBRATE",
        "RECORD_AUDIO",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://sleepapp.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-av",
        {
          "microphonePermission": "Esta app necesita acceso al micr칩fono para grabar sonidos del sue침o"
        }
      ],
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "extra": {
      "router": {
        "origin": "https://sleepapp.com/"
      },
      "eas": {
        "projectId": "afb97b42-a2bd-4f96-9be6-ab2f4879391d"
      }
    }
  }
}
